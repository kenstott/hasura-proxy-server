{
  "name": "hasura-proxy-server",
  "version": "1.0.0",
  "description": "Wraps a Hasura Server in an Apollo Server to provide ability to define additional directives.",
  "main": "index.js",
  "scripts": {
    "deno:compile": "denoify",
    "deno:run": "deno task start",
    "node:compile": "tsc",
    "node:compile:watch": "tsc --watch",
    "node:debug:watch": "NODE_OPTIONS=--max-old-space-size=16384 run-p node:compile:watch node:debug",
    "node:debug": "nodemon -r dotenv/config --experimental-loader tsc-module-loader --inspect ./dist/index.js",
    "node:run": "node -r dotenv/config --experimental-loader tsc-module-loader --inspect ./dist/index.js",
    "sample:build:plugin": "npx ncc build src/plugins/data-anomalies-plugin/data-anomalies-plugin.ts -o custom-plugins/validator --target es2020 -m && npx ncc build src/plugins/profile-plugin/profile-plugin.ts -o custom-plugins/profiler --target es2020 -m ",
    "start": "npm run node:run",
    "postinstall": "chmod +x choose.sh",
    "docker:build:image": "docker build -t kstott/hasura-proxy-server .",
    "docker:run": "docker compose up",
    "initialize-models": "./.venv/bin/python ./src/python-scripts/initialize-models.py"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@graphql-tools/schema": "^10.0.3",
    "@graphql-tools/utils": "^10.1.2",
    "@grpc/grpc-js": "^1.10.8",
    "@grpc/proto-loader": "^0.7.13",
    "@grpc/reflection": "^1.0.4",
    "@json2csv/node": "^7.0.6",
    "@open-rpc/schema-utils-js": "^2.0.3",
    "@open-rpc/server-js": "^1.9.5",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.49.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.49.1",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@opentelemetry/sdk-trace-base": "^1.22.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "apache-arrow": "^16.1.0",
    "arquero": "^5.4.0",
    "change-case": "^5.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "flat": "^6.0.1",
    "google-protobuf": "^3.21.2",
    "gql-to-json-schema": "^1.0.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "graphql-to-json-schema": "^1.0.0",
    "into-stream": "^8.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.5.0",
    "object-hash": "^3.0.0",
    "pb-util": "^1.0.3",
    "python-shell": "^5.0.0",
    "ts-deepmerge": "^7.0.0",
    "uuid": "^9.0.1",
    "wordnet": "^2.0.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dataframe-js": "^1.4.3",
    "@types/deep-diff": "^1.0.5",
    "@types/google-protobuf": "^3.15.12",
    "@types/graphql-fields": "^1.3.9",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@types/object-hash": "^3.0.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@vercel/ncc": "^0.38.1",
    "denoify": "^1.6.9",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.0",
    "eslint-config-love": "^44.0.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "tsc-module-loader": "^0.0.1",
    "typescript": "^5.4.3"
  },
  "overrides": {
    "apache-arrow": "^16.1.0"
  },
  "browserslist": [
    "maintained node versions"
  ]
}
